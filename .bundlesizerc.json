{
  "files": [
    {
      "path": "dist/assets/index-*.js",
      "maxSize": "500kb",
      "compression": "gzip"
    },
    {
      "path": "dist/assets/vendor-*.js",
      "maxSize": "2mb",
      "compression": "gzip"
    },
    {
      "path": "dist/assets/index-*.css",
      "maxSize": "100kb",
      "compression": "gzip"
    },
    {
      "path": "dist/assets/*",
      "maxSize": "2.5mb",
      "compression": "gzip"
    }
  ],
  "ci": {
    "trackBranches": ["main", "develop"],
    "repoBranchBase": "main"
  }
}